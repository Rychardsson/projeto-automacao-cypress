{
  "name": "projeto-automacao-cypress",
  "version": "1.0.0",
  "description": "Projeto de Automação com Cypress + SQLite - CRUD de Fornecedores",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:run": "cypress run",
    "setup": "node server/database.js",
    "reset": "node server/reset-database.js",
    "ci": "npm run setup && npm run test",
    "ci:start": "node server/index.js",
    "ci:test": "cypress run --config baseUrl=http://localhost:3000 --browser electron",
    "ci:full": "npm run setup && node scripts/ci-test.js"
  },
  "keywords": [
    "cypress",
    "automation",
    "sqlite",
    "crud",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "cypress": "^13.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-cypress": "^5.1.0",
    "nodemon": "^3.0.1"
  }
}
